{
  "name": "estridi",
  "version": "1.0.0-beta005",
  "description": "",
  "bin": "./dist/esm/cli.js",
  "type": "module",
  "scripts": {
    "start": "tsx src/cli.ts -r generator -t vitest -i '../src/runner.js' && tsx src/cli.ts -r runner -t vitest -i '../src/runner.js'",
    "build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json && node cjsfix.js",
    "test": "vitest run",
    "tsc": "tsc",
    "prettier": "prettier -w **/*",
    "examples": "tsx examples.ts",
    "publish": "npm publish --tag beta"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "figma-api": "^1.11.0",
    "vitest": "^2.0.5",
    "tsx": "^4.19.1"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "typescript": "^5.6.2",
    "prettier": "^3.3.3",
    "@vitest/coverage-v8": "^2.1.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "exports": {
    ".": {
      "import": "./dist/esm/runner.js",
      "types": "./dist/esm/runner.d.ts",
      "require": "./dist/cjs/runner.js"
    }
  },
  "files": [
    "dist"
  ]
}
